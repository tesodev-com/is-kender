{
  "name": "component-library",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "sideEffets": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js"
    },
    "./button": {
      "types": "./dist/components/Button.d.ts",
      "import": "./dist/components/Button.js"
    },
    "./accordion": {
      "types": "./dist/components/Accordion.d.ts",
      "import": "./dist/components/Accordion.js"
    },
    "./alert": {
      "types": "./dist/components/Alert.d.ts",
      "import": "./dist/components/Alert.js"
    },
    "./badge": {
      "types": "./dist/components/Badge.d.ts",
      "import": "./dist/components/Badge.js"
    },
    "./breadcrumb": {
      "types": "./dist/components/Breadcrumb.d.ts",
      "import": "./dist/components/Breadcrumb.js"
    },
    "./col": {
      "types": "./dist/components/Col.d.ts",
      "import": "./dist/components/Col.js"
    },
    "./container": {
      "types": "./dist/components/Container.d.ts",
      "import": "./dist/components/Container.js"
    },
    "./divider": {
      "types": "./dist/components/Divider.d.ts",
      "import": "./dist/components/Divider.js"
    },
    "./fileupload": {
      "types": "./dist/components/FileUpload.d.ts",
      "import": "./dist/components/FileUpload.js"
    },
    "./progressbar": {
      "types": "./dist/components/ProgressBar.d.ts",
      "import": "./dist/components/ProgressBar.js"
    },
    "./row": {
      "types": "./dist/components/Row.d.ts",
      "import": "./dist/components/Row.js"
    },
    "./skeleton": {
      "types": "./dist/components/Skeleton.d.ts",
      "import": "./dist/components/Skeleton.js"
    },
    "./svg": {
      "types": "./dist/components/Svg.d.ts",
      "import": "./dist/components/Svg.js"
    },
    "./text": {
      "types": "./dist/components/Text.d.ts",
      "import": "./dist/components/Text.js"
    },
    "./toast": {
      "types": "./dist/components/Toast.d.ts",
      "import": "./dist/components/Toast.js"
    },
    "./toggle": {
      "types": "./dist/components/Toggle.d.ts",
      "import": "./dist/components/Toggle.js"
    },
    "./sidebar": {
      "types": "./dist/components/Sidebar.d.ts",
      "import": "./dist/components/Sidebar.js"
    },
    "./link": {
      "types": "./dist/components/Link.d.ts",
      "import": "./dist/components/Link.js"
    },
    "./input": {
      "types": "./dist/components/Input.d.ts",
      "import": "./dist/components/Input.js"
    },
    "./checkbox": {
      "types": "./dist/components/Checkbox.d.ts",
      "import": "./dist/components/Checkbox.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "node scripts/pre-build.js && vue-tsc -p tsconfig.lib.json && vite build && node scripts/after-build.js",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "preview": "vite preview --port 3001",
    "lint:js": "eslint --fix",
    "lint:style": "stylelint \"**/*.{css,scss,html,vue}\" --ignore-path .gitignore --fix",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "vitest",
    "watch": "storybook dev -p 6006",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-dictionary": "style-dictionary build --config ./style-dictionary/config.json",
    "prepare": "husky",
    "create-component": "node scripts/generator.js"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.21.0",
    "@material-design-icons/svg": "^0.14.15",
    "@storybook/addon-actions": "^8.5.8",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/addon-links": "^8.5.8",
    "@storybook/manager-api": "^8.5.8",
    "@storybook/test": "^8.5.8",
    "@storybook/theming": "^8.5.8",
    "@storybook/vue3": "^8.5.8",
    "@storybook/vue3-vite": "^8.5.8",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.6",
    "@vitest/ui": "^3.0.6",
    "@volar/typescript": "^2.4.11",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "babel-loader": "^9.2.1",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^9.32.0",
    "glob": "^11.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.2",
    "sass": "^1.85.0",
    "storybook": "^8.5.8",
    "style-dictionary": "^4.3.3",
    "stylelint": "^16.14.1",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.11.1",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.1",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-lib-inject-css": "^2.2.1",
    "vitest": "^3.0.6",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^2.2.2"
  }
}